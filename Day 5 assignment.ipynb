   ],
   "source": [
    "list1 = [0,1,2,10,4,1,0,56,2,0,1,3,0,56,0,4]\n",
    "m = max(list1)\n",
    "\n",
    "def fun(x):\n",
    "    if(x == 0):\n",
    "        return m+1\n",
    "    return x\n",
    "\n",
    "list1.sort(key = fun)\n",
    "print(list1)"
